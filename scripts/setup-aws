oc new-project demo2-amq
oc create secret generic qdr-internal-cert --from-file=tls.crt=tls-certs/tls.aws.crt --from-file=tls.key=tls-certs/tls.aws.key --from-file=ca.crt=tls-certs/ca.crt
oc create configmap qdr-config --from-file=qdrouterd.conf.template=qdrouterd.conf.aws --dry-run -o yaml | oc apply -f -
oc apply -f qdr.aws.yaml
